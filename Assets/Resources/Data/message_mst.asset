    message: "\u7269\u8CC7\u304C\u5FA1\u5165\u7528\u3067\u3057\u3087\u3046\u304B\u3002\u9244\u7832\u306B\u9A0E\u99AC\u3001\u5FCD\u306A\u3069\u53D6\u308A\u63C3\u3048\u3066\u304A\u308A\u307E\u3059\u3088\u3002"
